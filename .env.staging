KEEP_TRACE_FILES=true

# ============================================================================
# TEST EXECUTION CONFIGURATION (STAGING)
# ============================================================================

# Execution Parameters (Staging)
MAX_WORKERS=30
TIMEOUT=60000
NAVIGATION_TIMEOUT=30000
ACTION_TIMEOUT=10000
EXPECT_TIMEOUT=5000
API_TIMEOUT=15000

# Retry Configuration (Staging)
RETRY_ATTEMPTS=2
RETRY_DELAY=1000
RETRY_BACKOFF=2
