LOG_FORMAT=json
LOG_RETENTION_DAYS=30
LOG_BACKUP_ENABLED=true
LOG_ENCRYPTION_ENABLED=true
LOG_COMPRESSION=gzip

# Debug & Development (ENABLED in Staging)
DEBUG_MODE=true
DEBUG_PORT=9222
INSPECT_BRK_PORT=9229
VERBOSE_LOGGING=true

