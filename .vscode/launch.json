{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "üåê Launch Chrome (8080)",
        "type": "chrome",
        "request": "launch",
        "url": "http://localhost:8080",
        "webRoot": "${workspaceFolder}",
        "sourceMaps": true
      },
      {
        "name": "üåê Launch Chrome (Angular 4200)",
        "type": "chrome",
        "request": "launch",
        "url": "http://localhost:4200",
        "webRoot": "${workspaceFolder}",
        "sourceMaps": true
      },
      {
        "name": "üîó Attach to Chrome (Debug Port 9222)",
        "type": "chrome",
        "request": "attach",
        "port": 9222,
        "webRoot": "${workspaceFolder}",
        "sourceMaps": true
      },
      {
        "name": "üß™ Playwright Test Debug (Chromium)",
        "type": "node",
        "request": "launch",
        "program": "${workspaceFolder}/node_modules/@playwright/test/cli.js",
        "args": ["test", "--project=chromium", "--trace", "on"],
        "cwd": "${workspaceFolder}",
        "console": "integratedTerminal",
        "internalConsoleOptions": "neverOpen"
      }
    ]
  }
  