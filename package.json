  },
  "scripts": {
    "test:cuc": "npx cucumber-js --profile default",
    "test:cuc:smoke": "npx cucumber-js --profile smoke",
    "test:cuc:regression": "npx cucumber-js --profile regression",
    "test:cuc:debug": "npx cucumber-js --profile debug",
    "test:debug": "npx playwright test --debug",
    "test:ci": "npx cucumber-js --profile ci --reporter=dot --reporter=html --reporter=allure-playwright",
    "test:headless": "npx playwright test --headless",
    "test:headed": "npx playwright test --headed",
    "test:local": "npx playwright test",
    "test:docker": "docker exec pw-dev npx playwright test",
    "dev": "tsx",
    "test": "playwright test",
    "pageObjects-chrome": "npx playwright test assertion.spec.ts --project=chromium",
    "pageObjects-firefox": "npx playwright test assertion.spec.ts --project=firefox",
    "pageObjects-all": "npm run pageObjects-chrome && npm run pageObjects-firefox",
    "allure:generate": "allure generate --clean allure-results -o allure-report",
    "allure:open": "allure open -p 5252 allure-report"
  },
